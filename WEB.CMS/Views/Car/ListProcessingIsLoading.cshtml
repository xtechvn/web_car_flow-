@using Entities.ViewModels.Car;
@{
    Layout = null;
}
@model List<CartoFactoryModel>
@if (Model != null && Model.Count > 0)
{
    <table class="data-table">
        <thead>
            <tr>
                <th>STT</th>
                <th>Giờ đăng ký online</th>
                <th>Tên khách hàng (Trại, đại lý)</th>
                <th>Tên lái xe</th>
                <th>Số điện thoại</th>
                <th>Biển số xe</th>
                <th>Tải trọng xe (kg)</th>
                <th>TT xe có mặt</th>
                <th>Nhóm tài</th>
                <th>Tình trạng</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="CartoFactory_@item.Id">
                    <td>@item.RecordNumber</td>
                    <td>@item.RegisterDateOnline</td>
                    <td>@item.CustomerName</td>
                    <td>@item.DriverName</td>
                    <td>@item.PhoneNumber</td>
                    <td>@item.VehicleNumber</td>
                    <td>@item.VehicleLoad</td>
                    <td>@item.VehicleStatusName</td>
                    <td>@item.LoadTypeName</td>
                    <td>
                        <div class="status-dropdown dropdown-toggle">
                            @item.LoadingStatusName
                        </div>
                    </td>
                </tr>
            }

        </tbody>
    </table>
}
else
{
    <table class="data-table">
        <thead>
            <tr>
                <th>STT</th>
                <th>Giờ đăng ký online</th>
                <th>Tên khách hàng (Trại, đại lý)</th>
                <th>Tên lái xe</th>
                <th>Số điện thoại</th>
                <th>Biển số xe</th>
                <th>Tải trọng xe (kg)</th>
                <th>TT xe có mặt</th>
                <th>Nhóm tài</th>
                <th>Tình trạng</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
}

